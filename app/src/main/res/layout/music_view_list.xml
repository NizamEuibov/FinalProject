<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="58dp"
    android:background="@color/background"
    android:paddingStart="15dp"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:ignore="RtlSymmetry">

    <TextView
        android:id="@+id/tvMusicList"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:textSize="17sp"
        android:textColor="@color/white"
        android:text="@string/tvsearch"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"

        />

    <ImageButton
        android:id="@+id/ibMusicList"
        android:layout_width="13dp"
        android:layout_height="13dp"
        android:src="@drawable/ic_minidownload"
        android:background="@drawable/artists"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:contentDescription="@string/todo"
        android:layout_marginBottom="12dp"
        />


    <TextView
        android:id="@+id/tvMusicArtist"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:textSize="14sp"
        android:text="@string/tvsearch"
        android:textColor="@color/bbColor"
        app:layout_constraintStart_toEndOf="@id/ibMusicList"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="10dp"
        android:layout_marginStart="10dp"
        />
    
    
    <ImageButton
        android:id="@+id/ibMusicListEnd"
        android:layout_width="27dp"
        android:layout_height="27dp"
        android:src="@drawable/ic_3point"
        android:background="@color/background"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:contentDescription="@string/todo"
        />




</androidx.constraintlayout.widget.ConstraintLayout>